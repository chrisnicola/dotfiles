[core]
  editor = vim
  excludesfile = /Users/chrisnicola/.gitignore
[user]
  name = Chris Nicola
	email = chris.nicola@wealthbar.com
[color]
  diff = auto
  ui = auto
  branch = auto
  status = auto
[diff]
  renames = true
[push] 
  default = tracking
[branch]
  autosetuprebase = always
[alias]
  st = status
  ps = push
  pu = pull
  ci = commit -v
  br = branch
  co = checkout
  cp = cherry-pick
  cia = commit -v -a
  df = diff
  dfr = diff -R
  lg = log -p
  rb = rebase
  spull = !git svn fetch && git svn rebase
  spush = !git svn dcommit
  gr = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
  ada = add -A .
[filter "indent"]
  clean = cat
  smudge = cat > ~/.tmp/temp

[merge]
  tool = vimdiff
[github]
  user = chrisnicola
[mergetool]
  keepBackup = false
[rerere]
  enabled = true
[filter "lfs"]
	clean = git lfs clean %f
	smudge = git lfs smudge %f
	required = true
